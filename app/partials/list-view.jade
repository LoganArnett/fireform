public-header
div#content
	div.container
		h2 Form {{}}
			p.action.button Get Code
			div#code
				code
					$('#myFormDiv').fireform('https://fireform.firebaseio.com/users/{{auth.user.uid}}/lists/{{ListView.id}}/formPosts');
			ul#messages(ng-show='ListView.formPosts')
				li(ng-repeat='list in ListView.formPosts | orderByPriority | reverse') {{list}}
			button(ng-click='logout()') Log Out